[alias]
	ci = commit
	co = checkout
	br = branch
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	lg = log --graph --color
	ft = fetch
	rb = rebase
	cf = config
	df = diff
	l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[user]
	name = Labi Kyo
	email = labikyo@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[merge]
	tool = opendiff
[branch]
	autosetuprebase = always
